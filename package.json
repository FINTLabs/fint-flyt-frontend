        "@mui/icons-material": "^5.6.2",
        "@mui/material": "^5.7.0",
        "@mui/system": "^5.7.0",
        "@mui/x-data-grid": "^5.10.0",
        "@navikt/ds-css": "^7.32.2",
        "@navikt/ds-react": "^7.32.2",
        "@testing-library/jest-dom": "^5.16.5",
        "@types/lodash": "^4.14.176",
        "@types/node": "^12.0.0",
        "@types/react": "^17.0.0",
        "@types/react-dom": "^18.2.6",
