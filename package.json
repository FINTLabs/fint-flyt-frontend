{
    "name": "fint-flyt-frontend",
    "version": "0.1.0",
    "private": true,
    "proxy": "http://localhost:8080",
    "homepage": ".",
    "engines": {
        "node": ">=22.0.0"
    },
    "scripts": {
        "start": "vite",
        "dev": "vite --profile",
        "build": "tsc && vite build",
        "serve": "vite preview",
        "test": "vitest",
        "cypress": "cypress run",
        "lint": "eslint --ext ts,tsx src",
        "forward": "sh ./startUpForwards.sh start",
        "stop-forward": "sh ./startUpForwards.sh stop"
    },
    "dependencies": {
        "@emotion/react": "^11.14.0",
        "@emotion/styled": "^11.14.1",
        "@mui/icons-material": "^5.6.2",
        "@mui/material": "^5.7.0",
        "@navikt/aksel-icons": "^7.35.1",
        "@navikt/ds-css": "^7.32.2",
        "@navikt/ds-react": "^7.32.2",
        "@testing-library/jest-dom": "^5.16.5",
        "@types/lodash": "^4.14.176",
        "@types/node": "^12.0.0",
        "@types/react": "18.2.79",
        "@types/react-dom": "^18.2.6",
        "@typescript-eslint/parser": "^5.60.0",
        "date-fns": "^4.1.0",
        "i18next": "^21.6.14",
        "lodash": "^4.17.21",
        "react": "^18.2.0",
        "react-dnd": "^14.0.4",
        "react-dnd-html5-backend": "^14.0.2",
        "react-dom": "^18.2.0",
        "react-hook-form": "7.65.0",
        "react-i18next": "^11.16.2",
        "react-router": "^7.9.4",
        "typescript": "4.9.5",
        "web-vitals": "^1.0.1"
    },
    "devDependencies": {
        "@typescript-eslint/eslint-plugin": "^5.60.0",
        "@vitejs/plugin-react": "^5.1.1",
        "cypress": "^12.14.0",
        "eslint": "^8.43.0",
        "prettier": "^3.4.2",
        "vite": "^7.2.7",
        "vitest": "^4.0.15"
    },
    "jest": {
        "testMatch": [
            "**/?(*.)+(test).[jt]s?(x)"
        ]
    },
    "eslintConfig": {
        "extends": [
            "react-app",
            "react-app/jest"
        ]
    },
    "eslintIgnore": [
        "**.cy.js"
    ],
    "browserslist": {
        "production": [
            ">0.2%",
            "not dead",
            "not op_mini all"
        ],
        "development": [
            "last 1 chrome version",
            "last 1 firefox version",
            "last 1 safari version"
        ]
    }
}
