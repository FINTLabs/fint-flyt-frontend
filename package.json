    },
    "devDependencies": {
        "@babel/plugin-proposal-private-property-in-object": "^7.21.11",
        "@typescript-eslint/eslint-plugin": "^5.60.0",
        "cypress": "^12.14.0",
        "eslint": "^8.43.0",
        "http-proxy-middleware": "^2.0.7",
        "prettier": "^3.4.2"
